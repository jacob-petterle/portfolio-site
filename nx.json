{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "main",
  "plugins": [
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      }
    },
    {
      "plugin": "@nx/jest/plugin",
      "options": {
        "targetName": "test"
      }
    },
    {
      "plugin": "@nx/next/plugin",
      "options": {
        "buildTargetName": "build",
        "devTargetName": "dev",
        "startTargetName": "start",
        "serveStaticTargetName": "serve-static"
      }
    }
  ],
  "targetDefaults": {
    "build": {
      "cache": true
    },
    "package": {
      "cache": true
    },
    "typecheck": {
      "cache": true
    }
  },
  "nxCloudAccessToken": "OWYyYjkwMzgtYzQwYS00Zjg2LWJlMGMtNmE4N2E2ODE3OGIyfHJlYWQtd3JpdGU="
